{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    //
    "target": "es5",
    "lib": ["es6"],
    "types": ["reflect-metadata", "styled-components-react-native"],
    "module": "commonjs",
    "moduleResolution": "node",
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    //
    "paths": {
      "@/models": ["data/models"],
      "@/protocols": ["data/protocols"]
    }
  }
}
